set top {{ top }}
set top_file {{ topFile }}
{% if topSimFile|length > 1 %}
set top_sim_file {{ topSimFile }}
{% endif %}
set project_name $top
puts "Vivado project $project_name"
set work_dir {{ workDir }}

set device    {{ device }}
set package   {{ package }}
set speed     {{ speed }}
set part      $device$package$speed

set jobs {{ jobs }}
set project_dir [file normalize ./$project_name]